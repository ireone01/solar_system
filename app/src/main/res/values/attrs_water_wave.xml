<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="shapeType" format="enum">
        <enum name="circle" value="1" />
        <enum name="water_drop" value="2" />
        <enum name="glass" value="3" />
        <enum name="heart" value="4" />
        <enum name="star" value="5" />
        <enum name="square" value="6" />
        <enum name="rectangle" value="7" />
        <enum name="triangle" value="8" />
        <enum name="diamond" value="9" />
    </attr>
    <attr name="progress" format="integer" />
    <attr name="max" format="integer" />
    <attr name="borderWidthSize" format="dimension" />
    <attr name="frontColor" format="color" />
    <attr name="behindColor" format="color" />
    <attr name="borderColor" format="color" />
    <attr name="strong" format="integer" />
    <attr name="textColor" format="color" />
    <attr name="animatorEnable" format="boolean" />
    <attr name="textHidden" format="boolean" />
    <attr name="shapePadding" format="dimension" />

    <declare-styleable name="CircularWaterWaveView">
        <attr name="progress" />
        <attr name="max" />
        <attr name="strong" />
        <attr name="frontColor" />
        <attr name="behindColor" />
        <attr name="borderColor" />
        <attr name="borderWidthSize" />
        <attr name="textColor" />
        <attr name="animatorEnable" />
        <attr name="shapeType" />
        <attr name="textHidden" />
        <attr name="shapePadding" />
    </declare-styleable>
</resources>